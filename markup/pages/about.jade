doctype html
html(class="no-js", lang="ru")

  include ../components/head/head
  include ../components/header/header
  include ../components/create/create
  include ../components/numbers/numbers
  include ../components/honestly/honestly
  include ../components/design/design
  include ../components/responsibility/responsibility
  include ../components/gallery/gallery
  head
    +head(head.defaults)
  body.page
    |%=symbols=%

    main.diamond-main.diamond-top

      +header().header-black

      +create()

      +numbers()

      +design()

      +honestly()

      +responsibility()

      +gallery()

    script(src='%=static=%js/separate-js/svg4everybody.min.js')
    script svg4everybody();

    //- Main scripts. You can replace it, but I recommend you to leave it here
    script(src='%=static=%js/main%=hash=%%=min=%.js')