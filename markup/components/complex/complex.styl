.complex
  padding-top 75px
  padding-bottom 75px
  &-wrapper
    display flex
    justify-content space-between
    align-items center
  &-left
    width 51.5873015873%
    h2
      color #000
      line-height 1.3
      margin-bottom 10px
      + span
        display block
        margin-bottom 40px
        color: rgba(0,0,0, 0.6)
        font-family $Medium
        font-size 14px
  &-items
    display flex
    flex-wrap wrap
    justify-content space-between
    margin-bottom -30px
  &-item
    width calc(50% - 10px)
    margin-bottom 30px
    img
      display block
      margin-bottom 10px
    span
      display block
      font-size 16px
      font-family $Semi
      margin-bottom 20px
    p
      color rgba(0,0,0,0.7)
      font-size 14px
      font-family $Reg
      line-height 1.55
  &-right
    width 45%