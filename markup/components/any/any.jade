mixin any(data)
  section.any
    .content
      .title
        h2 Поговорим о вашем проекте?   
      
      .any-form.form
        .form-row
          .form-group
            input(type="text" placeholder="Ваше имя")
          .form-group
            input(type="tel" placeholder="+7 (999) 999-99-99")
          .form-group
            button.button.button-black
              span Отправить заявку